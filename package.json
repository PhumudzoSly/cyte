{
  "name": "cyte",
  "version": "1.0.2",
  "description": "Recursive Web to Markdown CLI for AI Agents",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "cyte": "dist/cli.js",
    "cite": "dist/cli.js"
  },
  "scripts": {
    "build": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\" && tsc -p tsconfig.json",
