    "fs-extra": "^11.3.3",
    "jsdom": "^28.1.0",
    "ora": "^9.3.0",
    "p-limit": "^7.3.0",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "undici": "^7.22.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^27.0.0",
